{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "sourceMap": true,
      "noImplicitAny": true,
      "removeComments": true,
      "typeRoots": [
        "./types",
        "./node_modules/@types"
      ],
      "plugins": [
        {
          "transform": "ts-auto-mock/transformer",
          "cacheBetweenTests": false
        }
      ]
    },
    "exclude": [
      "node_modules",
      "types"
    ]
  }
